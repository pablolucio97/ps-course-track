{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@themes/*": ["themes/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@utils/*": ["utils/*"],
      "@contexts/*": ["contexts/*"],
      "@store/*": ["store/*"],
      "@data/*": ["data/*"],
      "@hooks/*": ["hooks/*"],
      "@services/*": ["services/*"],
      "@interfaces/*": ["interfaces/*"],
      "@routes/*": ["routes/*"],
      "@styles/*": ["styles/*"],
      "@appConstants/*": ["appConstants/*"]
    },
    "typeRoots": ["./node_modules/@types", "./@types"]
  }
}
